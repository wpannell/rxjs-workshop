{
  "name": "rxworkshop-1",
  "version": "1.0.0",
  "description": "a workshop for rxjs",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-push": "npm run server:lint && npm run server:spec"
    }
  },
  "scripts": {
    "prepush": "npm run server:lint && npm run server:spec",
    "start": "node exercises/demo-server/app.js",
    "server:lint": "esw -f simple-detail --clear server",
    "server:lint:watch": "npm run --silent server:lint -- -w",
    "server:start": "webpack --colors --progress --config webpack.server.config.js",
    "server:start:reload": "npm run server:start & browser-sync start --proxy 'localhost:3001' --files 'dist-server'",
    "server:spec": "NODE_ENV=TEST rimraf .tmp && mocha-webpack --webpack-config webpack.server.config.spec.js --reporter spec --growl --colors --timeout 10000 --require source-map-support/register server/spec/helper.js \"server/**/*.spec.js\"",
    "server:spec:watch": "npm run server:spec -- --watch",
    "tdd": "npm run server:spec:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "split-guide generate --templates-dir src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blesh/rxworkshop-1.git"
  },
  "author": "Ben Lesh <ben@benlesh.com>",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/blesh/rxworkshop-1/issues"
  },
  "homepage": "https://github.com/blesh/rxworkshop-1#readme",
  "dependencies": {
    "apollo-server-express": "1.1.6",
    "axios": "0.18.0",
    "bcrypt": "1.0.3",
    "bootstrap": "4.0.0",
    "core-js": "2.5.1",
    "cors": "^2.8.3",
    "express": "4.16.1",
    "express-jwt": "5.3.0",
    "graphql-tools": "2.4.0",
    "hammerjs": "2.0.8",
    "immutable": "3.8.2",
    "immutable-ext": "1.1.2",
    "jquery": "3.3.1",
    "jsonwebtoken": "8.1.0",
    "lodash.merge": "4.6.0",
    "moment": "2.20.1",
    "mongoose": "4.12.1",
    "popper.js": "1.14.1",
    "rxjs": "5.5.6"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.35",
    "@types/merge2": "^0.3.30",
    "@types/minimist": "^1.2.0",
